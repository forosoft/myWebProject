<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
    <class name="com.xyf.entity.RunoobTbl" table="BookCard">
        <id name="runoobId" column="runoobId">
            <generator class="native"></generator>
        </id>
        <property name="runoobTitle" column="runoobTitle"></property>
        <property name="runoobAuthor" column="runoobAuthor"></property>
        <property name="submissionDate" column="submissionDate"></property>
    </class>
</hibernate-mapping>